{
  "name": "playwright-palette-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Playwright automation API for generating Radix color palettes",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test:local": "curl -X POST http://localhost:3000/api/generate-palette -H 'Content-Type: application/json' -d '{\"hex\": \"#3B82F6\", \"scheme\": \"analogous\"}'"
  },
  "dependencies": {
    "@railway/cli": "^4.6.3",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "playwright": "^1.55.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.0"
  },
  "engines": {
    "node": ">=18"
  }
}
